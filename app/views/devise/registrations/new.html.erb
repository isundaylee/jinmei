<%= form_for(resource, html: {class: 'flatpack-form'}, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <h3>注册</h3>

  <%= devise_error_messages! %>

  <label><%= f.email_field :email, :autofocus => true, placeholder: '电子邮箱地址' %></label>
  <%= f.password_field :password, placeholder: '密码' %>
  <%= f.password_field :password_confirmation, placeholder: '确认密码' %>
  <%= f.button "注册", id: 'contact-submit' %>

<% end %>

